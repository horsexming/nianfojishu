		<!-- 【配置本流水卡片自身的对应关系(一对多)】 -->
		<set name="procardSet" cascade="all" inverse="false" order-by="id"
			lazy="true">
			<key column="fk_ProcardTId"></key>
			<one-to-many class="Procard" />
		</set>
		<!-- 【配置本流水卡片自身的对应关系(多对一)】 -->
		<many-to-one name="procard" class="Procard" cascade="save-update"
			lazy="no-proxy" column="fk_ProcardTId" />

		<!-- 【配置流水卡片下层工序信息关系】 -->
		<set name="processInforSet" inverse="false" cascade="all" lazy="true"
			order-by="processNO">
			<key column="fk_procardId"></key>
			<one-to-many class="ProcessInfor" />
		</set>

		<!-- 【配置流水卡片对应领料信息关系】 -->
		<set name="procardPartsSet" inverse="false" cascade="all" lazy="true">
			<key column="procard_id">
