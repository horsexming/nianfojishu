		</set>

		<set name="oneProcardBonus" inverse="false" cascade="none" lazy="true">
			<key column="procard_id">
			</key>
			<one-to-many class="com.task.entity.sop.OneProcardBonus" />
		</set>

		<!-- 【配置流水卡片对应领取工序成员信息关系】 -->
		<set name="processPeopleSet" inverse="false" cascade="all-delete-orphan" lazy="true"
			order-by="addTime">
			<key column="fk_procardId"></key>
			<one-to-many class="ProcessinforPeople" />
		</set>

		<!-- 【配置流水卡片对应采购计划信息关系】 -->
		<set name="wgwwPlanSet" inverse="false" cascade="all" lazy="true"
			order-by="addTime">
			<key column="fk_procardId"></key>
			<one-to-many class="WaigouWaiweiPlan" />
		</set>
		
