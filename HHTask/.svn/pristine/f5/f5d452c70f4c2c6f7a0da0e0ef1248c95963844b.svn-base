package com.task.util;

import java.util.Comparator;

import com.task.entity.Tijiang;

public class TJCompartor implements Comparator<Tijiang>{  
	 public int compare(Tijiang s1,Tijiang s2){
		  if(s1.equals(s2)){
		   return 0 ;
		  }else if(s1.getTjcount()<s2.getTjcount()){ // 按年龄比较
		   return 1 ;
		  }else{
		   return -1 ;
		  }
		 }

}
