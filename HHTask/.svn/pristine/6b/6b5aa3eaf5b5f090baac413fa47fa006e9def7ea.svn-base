package com.task.entity.payee;

import com.task.util.FieldMeta;

/****
 * @表名 ta_payee
 * @author Administrator
 *
 */
public class Payee implements java.io.Serializable{
	private Integer id;
	private String num;//编号
	private String code;//工号
	private String aNumber;//账号
	@FieldMeta(name = "收款单位名称")
	private String name;//名称
	private String bank;//开户行
	private String cmobile;//联系方式
	private String status;//状态
	private Integer epId;//审批ID
	private String epStattus;//审批状态
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getNum() {
		return num;
	}
	public void setNum(String num) {
		this.num = num;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getaNumber() {
		return aNumber;
	}
	public void setaNumber(String aNumber) {
		this.aNumber = aNumber;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getCmobile() {
		return cmobile;
	}
	public void setCmobile(String cmobile) {
		this.cmobile = cmobile;
	}
	public Integer getEpId() {
		return epId;
	}
	public void setEpId(Integer epId) {
		this.epId = epId;
	}
	public String getEpStattus() {
		return epStattus;
	}
	public void setEpStattus(String epStattus) {
		this.epStattus = epStattus;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public String getBank() {
		return bank;
	}
	public void setBank(String bank) {
		this.bank = bank;
	}
	
	
}
