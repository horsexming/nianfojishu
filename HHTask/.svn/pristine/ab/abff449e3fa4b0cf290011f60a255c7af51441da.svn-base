package com.task.entity.android;

import java.io.Serializable;

import com.task.entity.sop.WaigouDailySheet;

/***
 * 检验明细表
 * 
 * @author liupei
 * @表名 ta_m_OsRecordScope
 */
public class OsRecordScope implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private Integer id;
	private Integer jyCount;// 检验次数(统计本批次检验的件数)
	private String title;// 检查的标题
	private String content;// 巡检内容
	private OsScope scope;
	private Integer scopeId;
	private OsRecord osRecord;
	
	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public OsScope getScope() {
		return scope;
	}

	public void setScope(OsScope scope) {
		this.scope = scope;
	}

	public Integer getScopeId() {
		return scopeId;
	}

	public void setScopeId(Integer scopeId) {
		this.scopeId = scopeId;
	}

	public Integer getJyCount() {
		return jyCount;
	}

	public void setJyCount(Integer jyCount) {
		this.jyCount = jyCount;
	}

	public OsRecord getOsRecord() {
		return osRecord;
	}

	public void setOsRecord(OsRecord osRecord) {
		this.osRecord = osRecord;
	}




}
