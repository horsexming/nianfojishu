<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.task.entity.approval.Signature" table="ta_Signature">
		<id name="id">
			<generator class="identity"></generator>
		</id>
		<property name="code" />
		<property name="name" />
		<property name="signature_address" />
		<property name="default_address" />
		<property name="signature_date" />
	</class>
</hibernate-mapping>