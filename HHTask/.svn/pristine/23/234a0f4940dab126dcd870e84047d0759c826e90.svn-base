package com.task.entity.renshi;

import java.io.Serializable;
import java.util.Set;


public class InDoorScreen implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private Integer id;
	private String name;//屏幕名称
	private String position;//位置
	private String addTime;//添加时间
	private String updateTime;//更新时间
	
	
	private Set<InDoorScreenUsers> users; //绑定人员

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPosition() {
		return position;
	}

	public void setPosition(String position) {
		this.position = position;
	}

	
	

	public Set<InDoorScreenUsers> getUsers() {
		return users;
	}

	public void setUsers(Set<InDoorScreenUsers> users) {
		this.users = users;
	}

	public String getAddTime() {
		return addTime;
	}

	public void setAddTime(String addTime) {
		this.addTime = addTime;
	}

	public String getUpdateTime() {
		return updateTime;
	}

	public void setUpdateTime(String updateTime) {
		this.updateTime = updateTime;
	}

	
	
	
	
	
}
