		url(//misc.360buyimg.com/product/cosmetic/1.0.0/css/i/cosmetic_bg02.png)
		no-repeat;
	_background:
		url(//misc.360buyimg.com/product/cosmetic/1.0.0/css/i/cosmetic_bg02_ie6.gif)
		no-repeat
}

