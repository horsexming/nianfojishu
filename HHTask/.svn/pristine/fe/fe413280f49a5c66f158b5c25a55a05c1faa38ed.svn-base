#w-slider {
	height: 220px;
	background: #fff9e3;
	overflow: hidden;
	position: relative;
	display: block;
	-webkit-animation: sliderBanner 1s ease-in-out;
	-moz-animation: sliderBanner 1s ease-in-out;
	-ms-animation: sliderBanner 1s ease-in-out;
	-o-animation: sliderBanner 1s ease-in-out;
	animation: sliderBanner 1s ease-in-out
}

#w-slider .slide-close {
	width: 16px;
	height: 16px;
	position: absolute;
	top: 5px;
	right: 5px;
	z-index: 2;
	background:
		url(//misc.360buyimg.com/product/cosmetic/1.0.0/css/i/cosmetic_bg02.png)
		-38px 0 no-repeat;
	cursor: pointer
}

