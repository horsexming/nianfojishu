#categorys-2014 .dt,#search-2014 .button {
	background: #b80e5e
}

#categorys-2014 .dt {
	text-align: center
}

#search-2014 .text {
	border-color: #b80e5e
}

#nav-2014 {
	border-bottom-color: #b80e5e
}

#first-screen {
	position: relative;
	height: 420px
}

#first-screen .w {
	position: relative;
	z-index: 2
}

.p-slider .slider-wrap,.p-slider .slider-wrap ul {
	width: 100%
}

.p-slider .slider-wrap ul li {
	width: 100%;
	left: 0
}

.p-slider .slider-wrap .slider-img {
	width: 990px;
	margin: 0 auto;
	overflow: hidden
}

.p-slider .slider-trigger {
	position: absolute;
	z-index: 2;
	width: 100%;
	left: 0;
	top: 390px;
	height: 0;
	text-align: center
}

.p-slider .slider-trigger .w {
	height: 0
}

.p-slider .slider-trigger a {
	display: inline-block;
	width: 20px;
	height: 20px;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	background: #3d3d3d;
	font: 13px/ 20px arial;
	color: #fff;
	text-align: center
}

.p-slider .slider-trigger a.curr {
	background: #b80e5e;
	color: #dec6d3
}

.p-slider .slider-page {
	display: none;
	position: absolute;
	z-index: 2;
	width: 100%;
	left: 0;
	top: 198px
}

.p-slider .slider-page .w {
	position: relative
}

.p-slider .slider-page .next,.p-slider .slider-page .prev {
	position: absolute;
	display: block;
	height: 60px;
	width: 30px;
	text-align: center;
	font: 700 24px/ 60px simsun;
	filter: progid : DXImageTransform.Microsoft.gradient ( enabled = 'true',
		startColorstr = '#4C362C39', endColorstr = '#4C362C39' );
	background: rgba(54, 44, 57, .3); *
	background: url(_blank);
	color: #dec6d3
}

.p-slider .slider-page .prev:hover {
	filter: progid : DXImageTransform.Microsoft.gradient ( enabled = 'true',
		startColorstr = '#7F362C39', endColorstr = '#7F362C39' );
	background: rgba(54, 44, 57, .5)
}

.p-slider .slider-page .next {
	right: 0
}

.p-slider .slider-page .next:hover {
	filter: progid : DXImageTransform.Microsoft.gradient ( enabled = 'true',
		startColorstr = '#7F362C39', endColorstr = '#7F362C39' );
	background: rgba(54, 44, 57, .5)
}

