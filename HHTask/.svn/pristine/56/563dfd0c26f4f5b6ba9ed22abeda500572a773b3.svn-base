<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.task.entity.expresscabinet">
	<class name="Courier" table="ta_kdg_Courier">

		<id name="id">
			<generator class="identity"></generator>
		</id>
		<property name="couName"/>	
		<property name="phoneNumber"/>
		<property name="kdCompany"/>
		<property name="code"/>
		<property name="couReTime"/>
		<property name="idNumber"/>
		<property name="idFront"/>
		<property name="primaryName"/>
		
		
		
		<property name="couState"/>
		<property name="epId"/>
		
		
	</class>

</hibernate-mapping>
