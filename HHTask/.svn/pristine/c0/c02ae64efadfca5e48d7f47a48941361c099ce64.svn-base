<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.task.entity.Department" table="ST_Department">
		<id name="id" column="dept_id" type="int">
			<generator class="increment"></generator>
		</id>
		<property name="deptNo" column="dept_No" type="string"></property>
		<property name="name" column="dept_Name" type="string"></property>
		<property name="isDel" column="is_Del" type="int"></property>
	</class>
</hibernate-mapping>