package com.task.entity;

import java.io.Serializable;
import java.util.Set;

/***
 * 入职培训
 * 
 * @author 刘培
 * 
 */
public class JoinTrain  implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private Integer id;
	private String trainName;// 培训名称
	private String ownTrainProposal;// 本人意见和建议
	private String trainEffect;// 培训效果
	private Set<JoinTrainDetails> joinTDetails;
	private Users user;

	public JoinTrain() {
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getTrainName() {
		return trainName;
	}

	public void setTrainName(String trainName) {
		this.trainName = trainName;
	}

	public String getOwnTrainProposal() {
		return ownTrainProposal;
	}

	public void setOwnTrainProposal(String ownTrainProposal) {
		this.ownTrainProposal = ownTrainProposal;
	}

	public String getTrainEffect() {
		return trainEffect;
	}

	public void setTrainEffect(String trainEffect) {
		this.trainEffect = trainEffect;
	}

	public Set<JoinTrainDetails> getJoinTDetails() {
		return joinTDetails;
	}

	public void setJoinTDetails(Set<JoinTrainDetails> joinTDetails) {
		this.joinTDetails = joinTDetails;
	}

	public Users getUser() {
		return user;
	}

	public void setUser(Users user) {
		this.user = user;
	}
}
