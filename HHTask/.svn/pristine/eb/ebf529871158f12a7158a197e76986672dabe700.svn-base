<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.task.entity.Project" table="ta_Project">
		<id name="id">
			<generator class="identity"></generator>
		</id>
		<property name="name" length="30"/>
		<property name="numb" length="50"/>
		<property name="staring" length="100" />
		<property name="closed" type="true_false"/>
		<property name="createPerson" length="20"/>
		<property name="createDate" length="20"/>
	</class>

</hibernate-mapping>