<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.task.entity.bbs.BoardReview" table="b_board_review">
		<id name="id" type="integer" column="id">
			<generator class="identity"></generator>
		</id>
		<property name="content" type="text" column="content"/>
		<property name="createUserId" type="integer" column="create_user_id"/>
		<property name="createDate" type="timestamp" column="create_date"/>
		<property name="boardId" type="integer" column="board_id"/>
		<property name="parentId" type="integer" column="parent_id"/>
	</class>
</hibernate-mapping>