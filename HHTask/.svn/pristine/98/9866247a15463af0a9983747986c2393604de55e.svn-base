$(document).ready(function() {
	getCaizhi("wgType");
})
function update(id, cpage) {
	window.location.href = "WaigouwaiweiPlanAction!toAdjust.action?procard.id="
			+ id + "&cpage=" + cpage;
}
function clicks() {
