package com.task.entity.caiwu;

import java.io.Serializable;

/****
 * 财务账务表
 * 
 * @表名 ta_fin_CwZWAndSbr
 * @author 刘培
 * 
 */
public class CwZWAndSbr implements Serializable{

	private Integer id;// id主键
	private Integer fk_zwId;// 账务表id
	private Integer fk_sbrId;// 科目表id
	private String jiaOrJian;// 方向(+/-)
	private String dateType;// 时间类型(期初、期末)(qichuMoney/jieyuMoney)

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Integer getFk_zwId() {
		return fk_zwId;
	}

	public void setFk_zwId(Integer fkZwId) {
		fk_zwId = fkZwId;
	}

	public Integer getFk_sbrId() {
		return fk_sbrId;
	}

	public void setFk_sbrId(Integer fkSbrId) {
		fk_sbrId = fkSbrId;
	}

	public String getJiaOrJian() {
		return jiaOrJian;
	}

	public void setJiaOrJian(String jiaOrJian) {
		this.jiaOrJian = jiaOrJian;
	}

	public String getDateType() {
		return dateType;
	}

	public void setDateType(String dateType) {
		this.dateType = dateType;
	}

}
