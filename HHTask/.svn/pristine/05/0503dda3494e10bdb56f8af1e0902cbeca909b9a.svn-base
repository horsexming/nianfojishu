package com.task.entity.sop;

/***
 * 流水卡(表名:ta_sop_runningWaterCard)
 * 
 * @author jhh
 * 
 */
public class RunningWaterCard  implements java.io.Serializable{
	private static final long serialVersionUID =1L;

	private Integer id;// 主键
	private String cardNum;// 卡号,周转单条码
	private String customer;// 客户（型别）
	private String carStyle;// 车型
	private String markId;// 件号
	private String createCardTime;// 制卡时间
	private String creator;// 制卡者
	/**新添**/
	private String rwStyle;//（周转单，周转卡）
	private String productStyle;//生产类型（批产/试制）
	private String partName;//零件名称
	/**
	 * 以下属性是需要流转变动的
	 */
	private String cardStatus;// 卡跟踪状态（初始/已发卡/已发料/领工序/完成/入库）
	private String receiveStatus;// 领料状态(初始、NO、YES)
	private String followCardId;// 跟踪流水卡片的批次号
	private String followTime;// 发卡时间，跟踪开始时间
	private String ownUsername;// 卡目前的持有者
	private String lastProcessTime;// 最后工序提交时间
	private String more;// 备注
	private Integer procardId;// 流水单id

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getCardNum() {
		return cardNum;
	}

	public void setCardNum(String cardNum) {
		this.cardNum = cardNum;
	}

	public String getCustomer() {
		return customer;
	}

	public void setCustomer(String customer) {
		this.customer = customer;
	}

	public String getCarStyle() {
		return carStyle;
	}

	public void setCarStyle(String carStyle) {
		this.carStyle = carStyle;
	}

	public String getMarkId() {
		return markId;
	}

	public void setMarkId(String markId) {
		this.markId = markId;
	}

	public String getCreateCardTime() {
		return createCardTime;
	}

	public void setCreateCardTime(String createCardTime) {
		this.createCardTime = createCardTime;
	}

	public String getCreator() {
		return creator;
	}

	public void setCreator(String creator) {
		this.creator = creator;
	}

	public String getCardStatus() {
		return cardStatus;
	}

	public void setCardStatus(String cardStatus) {
		this.cardStatus = cardStatus;
	}

	public String getReceiveStatus() {
		return receiveStatus;
	}

	public void setReceiveStatus(String receiveStatus) {
		this.receiveStatus = receiveStatus;
	}

	public String getFollowCardId() {
		return followCardId;
	}

	public void setFollowCardId(String followCardId) {
		this.followCardId = followCardId;
	}

	public String getFollowTime() {
		return followTime;
	}

	public String getPartName() {
		return partName;
	}

	public void setPartName(String partName) {
		this.partName = partName;
	}

	public void setFollowTime(String followTime) {
		this.followTime = followTime;
	}

	public String getOwnUsername() {
		return ownUsername;
	}

	public void setOwnUsername(String ownUsername) {
		this.ownUsername = ownUsername;
	}

	public String getMore() {
		return more;
	}

	public void setMore(String more) {
		this.more = more;
	}

	public String getLastProcessTime() {
		return lastProcessTime;
	}

	public void setLastProcessTime(String lastProcessTime) {
		this.lastProcessTime = lastProcessTime;
	}

	public Integer getProcardId() {
		return procardId;
	}

	public void setProcardId(Integer procardId) {
		this.procardId = procardId;
	}

	public String getRwStyle() {
		return rwStyle;
	}

	public void setRwStyle(String rwStyle) {
		this.rwStyle = rwStyle;
	}

	public String getProductStyle() {
		return productStyle;
	}

	public void setProductStyle(String productStyle) {
		this.productStyle = productStyle;
	}
	
}
