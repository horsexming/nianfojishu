		<property name="gongchuPlace" />
		<property name="exitTime" />
		<property name="leaveUserCardId" />
		<property name="returnTime" />	
		<property name="huanxiustatus" />	
		<property name="singleCarId" />	
		<property name="leaveObjectType" />	
		<property name="leaveObjectNeirong" />	
		<property name="accessStatus" />
		<property name="carPaiNum" />
		<property name="rgcost" />
		<property name="freeDepts" />
		<property name="freeDeptIds" />
		<property name="sqStatus" />
		<property name="lng" length="3" scale="6"/>
		<property name="lat" length="3" scale="6"/>
		
	</class>
