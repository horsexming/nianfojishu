package com.task.entity;

import java.io.Serializable;

import com.task.entity.renshi.Dimission_XieYi;

/**
 * 合同条款类(表名:ta_hr_Provision)
 * 
 * @author 刘培
 * 
 */
public class Provision  implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private Integer id;
	private String content;// 内容
	private String provisionStatus;// 条款状态(public/private/zj)
	private Contract contract;// 合同
	private Dimission_XieYi dimissionXieYi;//离职协议
	private Integer num; //位置
	

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Dimission_XieYi getDimissionXieYi() {
		return dimissionXieYi;
	}

	public void setDimissionXieYi(Dimission_XieYi dimissionXieYi) {
		this.dimissionXieYi = dimissionXieYi;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getProvisionStatus() {
		return provisionStatus;
	}

	public void setProvisionStatus(String provisionStatus) {
		this.provisionStatus = provisionStatus;
	}

	public Contract getContract() {
		return contract;
	}

	public void setContract(Contract contract) {
		this.contract = contract;
	}

	public void setNum(Integer num) {
		this.num = num;
	}

	public Integer getNum() {
		return num;
	}

}
