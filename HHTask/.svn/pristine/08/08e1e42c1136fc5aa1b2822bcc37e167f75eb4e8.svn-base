			}
			.resQRCode,.leaveRQCode,.codeBookRQCode,.dutyRQCode{
/* 				height: 60px; */
				width: 100%;
				text-align: center;
			}
			.visitorIcon{
				width: 25px;
				height: 25px;
				display: inline-block;
				border: solid 1px #000000;
 				border-radius: 50%;
 				background-color: #529DD5;
 				margin-right: 10px;
			}
			#showMessage{
				height: 30px;
				line-height: 30px;
				font-size: 20px;
			}
			/*菜谱*/
			#codebookUl{
				list-style-type: none;
				font-size: 15px;
				margin: 0 auto;
				height: 380px;
				overflow: hidden;
			}
			#codebookUl li{
   				list-style:none;
   				padding-bottom: 5px;
			}
			#codebookUl img{
				width:50px;
				height:50px;
				
			}
			
			#title{
				height: 20%;
			}
			.oneTr{
				height: 30%;
			}
			.twoTr{
				height: 30%;
			}
			.userList{
				height:65%; 
			}
			#QRCodeParent{
				padding: 0;
				margin: 0;
				border: none;
			}
			#QRCode,#QRCode tr{
				border-top:none;
				border-bottom:none;
				border:none;
				padding: 0;
				margin: 0;
				background-color: #97BCE4;
				border-collapse: collapse;table-layout: fixed;
			}
			#QRCode td:first-child{
				border-left: none;
			}
			#QRCode td:last-child{
				border-right: none;
			}
			#QRCode td{
				border-top:none;
