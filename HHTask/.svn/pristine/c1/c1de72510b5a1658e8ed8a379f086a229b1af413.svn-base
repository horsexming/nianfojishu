package com.task.entity.sop.cb;

import java.io.Serializable;

/**
 * @author wxf
 *procard成本动态表:(ta_cb_ProcardCBDT)
 */
public class ProcardCBDT implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private Integer id;
	private Integer pId;//procardId;
	private Integer processId;//工序id
	private Float addCB;//新增成本;
	private Float sumCB;//总成本;
	private String timeCB;//每次新增成本时间(yyyy-MM-dd HH:mm:ss);
	private String cbType;//人工、设备折旧、能源消耗、外购费、外委费、辅料费、管理费、差旅费
	private String more;//描述(详细说明每次成本的变化);
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getpId() {
		return pId;
	}
	public void setpId(Integer pId) {
		this.pId = pId;
	}
	public Float getAddCB() {
		return addCB;
	}
	public void setAddCB(Float addCB) {
		this.addCB = addCB;
	}
	public Float getSumCB() {
		return sumCB;
	}
	public void setSumCB(Float sumCB) {
		this.sumCB = sumCB;
	}
	
	public String getTimeCB() {
		return timeCB;
	}
	public void setTimeCB(String timeCB) {
		this.timeCB = timeCB;
	}
	public String getMore() {
		return more;
	}
	public void setMore(String more) {
		this.more = more;
	}
	public String getCbType() {
		return cbType;
	}
	public void setCbType(String cbType) {
		this.cbType = cbType;
	}
	public Integer getProcessId() {
		return processId;
	}
	public void setProcessId(Integer processId) {
		this.processId = processId;
	}
	
	
}
