package com.task.action.sop;

import java.util.List;

import com.opensymphony.xwork2.ActionSupport;

public class PtCNFXAction extends ActionSupport{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private List<String> checkbox;
	private List<String> markIdS;
	private List<String> capacityS;
	private int size ;
	
	
	public String changnengfenxi(){
		if(checkbox!=null && checkbox.size()>0){
			size = checkbox.size();
			return "Pt_cnfx";
		}
		return "";
	}
	
	
	public List<String> getMarkIdS() {
		return markIdS;
	}


	public void setMarkIdS(List<String> markIdS) {
		this.markIdS = markIdS;
	}


	public List<String> getCapacityS() {
		return capacityS;
	}


	public void setCapacityS(List<String> capacityS) {
		this.capacityS = capacityS;
	}


	public int getSize() {
		return size;
	}


	public void setSize(int size) {
		this.size = size;
	}


	public List<String> getCheckbox() {
		return checkbox;
	}

	public void setCheckbox(List<String> checkbox) {
		this.checkbox = checkbox;
	}

	
	
	
	
	
	
}
