<%@ page language="java" import="java.util.*,com.task.entity.*"
	pageEncoding="utf-8"%>
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<HTML>
	<HEAD>
		<TITLE>生产力生态平衡系统后台登录</TITLE>
		<link rel="shortcut icon" href="../favicon.ico" />
		<META http-equiv=Content-Type content="text/html; charset=gb2312">
		<LINK href="css/admin.css" type="text/css" rel="stylesheet">
		<script type="text/javascript">
		function message(){
		}
</script>
	</HEAD>
	<BODY onload="message()" bgcolor="#002779">

		<TABLE height="100%" cellSpacing=0 cellPadding=0 width="100%" bgcolor="#002779">
			<TR>
				<TD align=middle>
					<TABLE cellSpacing=0 cellPadding=0 width=468 border=0>
						<TR>
							<TD>
								<IMG height=23 src="images/login_1.jpg" width=468>
							</TD>
						</TR>
						<TR>
							<TD>
								<IMG height=147 src="images/login_2.jpg" width=468>
							</TD>
						</TR>
					</TABLE>
					<TABLE cellSpacing=0 cellPadding=0 width=468 bgColor=#ffffff
						border=0>
						<TR>
							<TD width=16>
								<IMG height=122 src="images/login_3.jpg" width=16>
							</TD>
							<TD align=middle>
								<form action="AdminAction!adminLogin.action" method="post">
									<TABLE cellSpacing=0 cellPadding=0 border=0>
										<TR height=36>
											<TD>
												用户名:
											</TD>
											<TD>
												&nbsp;
												<INPUT style="BORDER: #000000 1px solid" maxLength=30
													size=26 name="admin.adminName">

											</TD>
											<TD>
												&nbsp;&nbsp;<font color='red'>*${message}</font>
											</TD>
										</TR>
										<TR height=36>
											<TD>
												密&nbsp;&nbsp;&nbsp;码:
											</TD>
											<TD>
												&nbsp;
												<INPUT style="BORDER: #000000 1px solid" type=password
													maxLength=30 size=26 name="admin.adminPassword">
												<label id="password"></label>
											</TD>
											<TD>
												&nbsp;&nbsp;<font color='red'>*${message}</font>
											</TD>
										</TR>
										<TR height=5>
											<TD colSpan=3></TD>
										</TR>
										<TR>
											<TD>
												&nbsp;
											</TD>
											<TD align="center">
												<input type="submit" value="登录"
													style="width: 70px; height: 30px">
												<input type="reset" value="重置"
													style="width: 70px; height: 30px">
											</TD>
											<TD>

											</TD>
										</TR>
									</TABLE>
								</form>
							</TD>
							<TD width=16>
								<IMG height=122 src="images/login_4.jpg" width=16>
							</TD>
						</TR>
					</TABLE>
					<TABLE cellSpacing=0 cellPadding=0 width=468 border=0>
						<TR>
							<TD>
								<IMG height=16 src="images/login_5.jpg" width=468>
							</TD>
						</TR>
					</TABLE>
				</TD>
			</TR>
		</TABLE>
	</BODY>
