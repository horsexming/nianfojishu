<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.task.entity.Bonusmoney" table="ta_hr_jjfp_bonusmoney">
		<id name="id" type="java.lang.Integer">
			<generator class="identity"></generator>
		</id>
		<property name="bonusmoneymonth" type="java.lang.String">
			<column name="bonusmoney_month"></column>
		</property>
		<property name="bonusmoneytotalmoney" type="java.lang.Float">
			<column name="bonusmoney_totalmoney" precision="53" scale="0"></column>
		</property>
		<property name="bonusmoneyteam" type="java.lang.String">
			<column name="bonusmoney_team"></column>
		</property>
		<property name="bonusmoneystatus" type="java.lang.String">
			<column name="bonusmoney_status"></column>
		</property>
		<property name="bonusmoneyname" type="java.lang.String">
			<column name="bonusmoney_name"></column>
		</property>
		<property name="bonusmoneydatatime" type="java.lang.String">
			<column name="bonusmoney_datatime"></column>
		</property>
		<property name="addUserId" />
		<property name="epId" />
	</class>
</hibernate-mapping>
