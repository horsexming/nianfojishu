<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.task.entity.sop.fhyp.FanghuYongpin" table="ta_sop_fhyp_FanghuYongpin">
        <id name="id" type="integer" column="id">
			<generator class="identity"></generator>
		</id>
		<!--  防护用品名称-->
		<property name="fanghuYongpinName" type="string" column="fanghu_yongpin_name"/>
		<!--  防护用品规格-->
		<property name="fanghuYongpinGuige" type="string" column="fanghu_yongpin_guige"/>
		<!--  所属防护用品ID-->
		<property name="parentId" type="integer" column="parent_id"/>
    </class>
</hibernate-mapping>
