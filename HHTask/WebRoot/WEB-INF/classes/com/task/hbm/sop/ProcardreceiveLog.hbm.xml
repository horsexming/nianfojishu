<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.task.entity.sop">
	<class name="ProcardreceiveLog" table="ta_ProcardreceiveLog">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="procardId" />
	<property name="sellId" />
	<property name="markId" />
	<property name="selfCard" />
	<property name="sell_lot" />
	<property name="sellCount" />
	<property name="unit" />

	</class>
</hibernate-mapping>
