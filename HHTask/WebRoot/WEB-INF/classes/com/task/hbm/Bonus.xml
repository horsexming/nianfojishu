<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.task.entity.Bonus" table="ta_hr_jjfp_bonus">
		<id name="id" type="java.lang.Integer">
			<generator class="identity"></generator>
		</id>
		<property name="bonusteam" type="java.lang.String">
			<column name="bonus_team"></column>
		</property>
		<property name="bonusteamname" type="java.lang.String">
			<column name="bonus_teamname"></column>
		</property>
		<property name="bonusmembermoney" type="java.lang.Float">
			<column name="bonus_membermoney" precision="53" scale="0"></column>
		</property>
		<property name="shizhiMoney" />
		<property name="bonusovertimemealmoney" type="java.lang.Float">
			<column name="bonus_overtimemealmoney" precision="53" scale="0"></column>
		</property>
		<property name="bonusmembernumber" type="java.lang.String">
			<column name="bonus_membernumber"></column>
		</property>
		<property name="bonuscardnumber" type="java.lang.String">
			<column name="bonus_cardnumber"></column>
		</property>
		<property name="bonusdata" type="java.lang.String">
			<column name="bonus_data"></column>
		</property>
		<property name="bonusdatatime" type="java.lang.String">
			<column name="bonus_datatime"></column>
		</property>
		<property name="fk_MoneyId" />
	</class>
</hibernate-mapping>
