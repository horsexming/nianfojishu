<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.task.entity.TbBarcode">
	<class name="TbBarcodeLockNotice" table="ta_TbBarcodeLockNotice">
		<id name="id">
			<generator class="identity"></generator>
		</id>
		<property name="barcode"/>
		<property name="matCode"/>
		<property name="matName"/>
		<property name="locked"/>
		<property name="lockReason"/>
		<property name="unlockReason"/>
		<property name="lockUser"/>
		<property name="lockDate"/>
		<property name="lockNo"/>
		<property name="status"/>
		<property name="fileName"/>
	</class>
</hibernate-mapping>
