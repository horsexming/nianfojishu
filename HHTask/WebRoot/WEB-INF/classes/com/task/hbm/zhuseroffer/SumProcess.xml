<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
	<class name="com.task.entity.zhuseroffer.SumProcess" table="ta_sumProcess" >
		<id name="id">
			<generator class="identity"/>
		</id>
		<property name="title"/>
		<property name="rootMark"/>
		<property name="ywmarkId"/>
		<property name="markId"/>
		<property name="addTime"/>
		<property name="count"/>
		<property name="stutas"/>
		<property name="bjStartDate"/>
		<property name="bjEndDate"/>
		<property name="cycle"/>
	</class>
</hibernate-mapping>