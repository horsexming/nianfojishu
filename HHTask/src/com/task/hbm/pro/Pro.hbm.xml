<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.task.entity.pro.Pro" table="p_pro">
		<id name="id" type="integer" column="id">
			<generator class="identity"></generator>
		</id>
		<property name="name" type="string" column="name"/>
		<property name="code" type="string" column="code"/>
		<property name="status" type="string" column="status"/>
		<property name="createUserId" type="integer" column="create_user_id"/>
		<property name="createDate" type="timestamp" column="create_date"/>
		<property name="clientId" type="integer" column="client_id"/>
		<property name="clientName" type="string" column="client_name"/>
		<property name="budget" type="double" column="bugget"/>
		<property name="finishDate" type="timestamp" column="finish_date"/>
		<property name="epId" />
		<property name="epStatus" />
		<property name="cgMoney" />
		<property name="peopleMoney" />
	</class>
</hibernate-mapping>