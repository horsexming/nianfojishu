<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.task.entity.pro.ProTooling" table="p_pro_tooling">
		<id name="id" type="integer" column="id">
			<generator class="identity"></generator>
		</id>
		<property name="name" type="string" column="name"/>
		<property name="numb" type="string" column="numb"/>
		<property name="amount" type="double" column="amount"/>
		<property name="specification" type="string" column="specification"/>
		<property name="status" type="string" column="status"/>
		<property name="proId" type="integer" column="pro_id"/>
		<property name="explain" type="string" >
		    <column name="[explain]" />
		</property>
	</class>
</hibernate-mapping>