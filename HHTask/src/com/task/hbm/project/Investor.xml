<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.task.entity.project">
	<class name="Investor" table="ta_pro_Investor">
		<id name="id">
			<generator class="identity" />
		</id>
	<property name="userId" />
	<property name="name" />
	<property name="code" />
	<property name="dept" />
	<property name="type" />
	<property name="cardNumber" />
	<property name="hasMoney" />
	<property name="djMoney" />
	<property name="kyMoney" />
	<property name="zanCount" />
	<property name="tocaoCount" />
	</class>
</hibernate-mapping>
