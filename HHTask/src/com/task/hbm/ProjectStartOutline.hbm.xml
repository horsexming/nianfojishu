<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.task.entity.ProjectStartOutline" table="ta_ProjectStart_Outline">
		<id name="id">
			<generator class="identity"></generator>
		</id>
		<property name="projectType" />
		<property name="implementationCycle" />
		<property name="productGraphics" length="80" />
		<many-to-one name="root" column="f_projectstart_id" lazy="false"/>
	</class>

</hibernate-mapping>