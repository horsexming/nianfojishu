<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.task.entity.jxkh">
	<class name="SalesAmountCoefficient" table="ta_SalesAmountCoefficient">
		<id name="id">
			<generator class="identity"></generator>
		</id>
		 <property name="lowAmount" not-null="true"/>
		 <property name="ceilingAmount"/>
		 <property name="coefficient" not-null="true"/>
		 <property name="addTime"/>
		 <property name="addUsersName"/>
		 <property name="danwei"/>
	</class>
</hibernate-mapping>
