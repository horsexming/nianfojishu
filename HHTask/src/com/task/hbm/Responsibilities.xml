<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.task.entity.Responsibilities" table="ta_xinxi_dutylist">
		<id name="id">
			<generator class="identity"></generator>
		</id>
		<property name="employeenumber" length="50" />
		<property name="repairname" length="50" />
		<property name="repairdepartment" length="50" />
		<property name="repaircategory" length="100" />
		<property name="repairresponsibilitiesl" length="500" />
		<property name="phone" length="50"/>
		<property name="mailbox" length="100"/>
	</class>

</hibernate-mapping>