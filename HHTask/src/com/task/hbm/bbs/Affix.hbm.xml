<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.task.entity.bbs.Affix" table="b_affix">
		<id name="id" type="integer" column="id">
			<generator class="identity"></generator>
		</id>
		<property name="fileName" type="string" column="file_name"/>
		<property name="realFileName" type="string" column="real_file_name"/>
		<property name="fileType" type="string" column="file_type"/>
		<property name="path" type="string" column="path"/>
		<property name="createDate" type="timestamp" column="create_date"/>
	</class>
</hibernate-mapping>