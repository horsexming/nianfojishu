		<property name="tanianzhejiudepreciation">
			<column name="ta_nianzhejiudepreciation" length="200"></column>
		</property>
		<property name="taspecificationsmodel">
			<column name="ta_specificationsmodel" length="200"></column>
		</property>
		<property name="tastartdatetime">
			<column name="ta_startdatetime" length="200"></column>
		</property>
		<property name="tanumber">
