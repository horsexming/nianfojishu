		</property>
		<property name="tascrappedwilling">
		<column name="ta_scrappedwilling" length="500"></column>
		</property>
		
		<property name="taequipmentModel">
		<column name="ta_equipmentModel" length="200"></column>
		</property>
		
		<property name="tamanufacturer">
		<column name="ta_manufacturer" length="200"></column>
		</property>
		
		<property name="tafactorynumber">
		<column name="ta_factorynumber" length="200"></column>
		</property>
		<property name="tafactorydate">
		<column name="ta_factorydate" length="200"></column>
		</property>
		
		<property name="tainstallationsite">
		<column name="ta_installationsite" length="200"></column>
		</property>
		
		<property name="tainstallationdate">
		<column name="ta_installationdate" length="200"></column>
		</property>
		
