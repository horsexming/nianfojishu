		</property>
		<property name="taaccumulateddepreciation">
			<column name="ta_accumulateddepreciation" length="200"></column>
		</property>
		<property name="taassetsNetworth">
			<column name="ta_assetsNetworth" length="200"></column>
		</property>
		<property name="taassetsQuantity">
