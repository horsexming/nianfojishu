		</property>
		<property name="tayearsof">
			<column name="ta_yearsof" length="200"></column>
		</property>
		<property name="tamonthlydepreciation">
			<column name="ta_monthlydepreciation" length="200"></column>
		</property>
		<property name="tayuezhejiudepreciation">
			<column name="ta_yuezhejiudepreciation" length="200"></column>
		</property>
		<property name="taannualrate">
			<column name="ta_annualrate" length="200"></column>
