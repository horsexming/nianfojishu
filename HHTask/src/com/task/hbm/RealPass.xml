<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.task.entity.PassReal" table="ta_pr">
		<id name="id">
			<generator class="identity"></generator>
		</id>
		<property name="realPass" length="50" column="ta_realPass" not-null="true"></property>
		<property name="uid" column="ta_uid"></property>
		<property name="mailPass" />
	</class>

</hibernate-mapping>
