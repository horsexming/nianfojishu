<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="com.task.entity.MessagePolling" table="ta_messagePolling">
        <id name="id">
            <generator class="identity"></generator>
        </id>
        <property name="addDate" />
        <property name="triggerDate" />
        <property name="messageType" />
        <property name="sendUsersId" />
        <property name="sendUsersName" />
        <property name="receiveUsersId" />
        <property name="receiveUsersName" />
        <property name="message" />
        <property name="messageTitle" />
        <property name="sendUsersCode" />
        <property name="receiveUsersCode" />
    </class>
</hibernate-mapping>