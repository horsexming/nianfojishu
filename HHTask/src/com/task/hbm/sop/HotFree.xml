<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.task.entity.sop">
	<class name="HotFree" table="ta_sop_HotFree">
		<id name="id">
			<generator class="identity"></generator>
		</id>
		<property name="userId" />
		<property name="name" />
	<property name="code" />
	<property name="dept" />
	<property name="time" />
	<property name="month" />
	<property name="type" />
	<property name="money" />
	<property name="qqMoney" />
	<property name="cqCount" />
	<property name="qqCount" />
	<property name="sfMoney" />

	</class>
</hibernate-mapping>
